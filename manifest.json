{
  "name": "Fresh Chrome Extension",
  "description": "Fully functional, useful bare minimum Google Chrome extension boilerplate [Manifest V3].",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Generate random pax number",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "run_at" : "document_idle",
      "js": ["after.js"],
      "matches": [
        "https://vamsys.io/*",
        "https://dispatch.simbrief.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
  ]
}
